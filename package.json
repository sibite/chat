{
  "name": "social-app",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/react": "^2.3.1",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@heroicons/react": "^1.0.6",
    "@reduxjs/toolkit": "^1.8.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/downscale": "^1.0.1",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.56",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "axios": "^1.2.0",
    "dayjs": "^1.11.5",
    "downscale": "^1.0.6",
    "express": "^4.18.1",
    "express-fileupload": "^1.4.0",
    "framer-motion": "^7.2.1",
    "http-proxy-middleware": "^2.0.6",
    "jsonwebtoken": "^8.5.1",
    "nedb": "^1.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "sharp": "^0.31.0",
    "socket.io": "^4.5.2",
    "socket.io-client": "^4.5.2",
    "typescript": "^4.8.2",
    "validator": "^13.7.0",
    "web-vitals": "^2.1.4",
    "workbox-background-sync": "^6.5.4",
    "workbox-broadcast-update": "^6.5.4",
    "workbox-cacheable-response": "^6.5.4",
    "workbox-core": "^6.5.4",
    "workbox-expiration": "^6.5.4",
    "workbox-google-analytics": "^6.5.4",
    "workbox-navigation-preload": "^6.5.4",
    "workbox-precaching": "^6.5.4",
    "workbox-range-requests": "^6.5.4",
    "workbox-routing": "^6.5.4",
    "workbox-strategies": "^6.5.4",
    "workbox-streams": "^6.5.4"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/public/static/js/*.js'",
    "start": "react-scripts start",
    "start-build": "node ./build/server",
    "dev": "npm run server | npm start",
    "server": "tsc -p ./server --watch | nodemon -w ./build/server ./build/server",
    "build": "react-scripts build",
    "postbuild": "npm run build-server",
    "build-server": "tsc -p ./server",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "airbnb",
      "airbnb-typescript",
      "prettier"
    ],
    "parserOptions": {
      "project": [
        "tsconfig.json",
        "server/tsconfig.json"
      ]
    },
    "ignorePatterns": [
      "src/serviceWorkerRegistration.ts",
      "src/service-worker.ts"
    ],
    "rules": {
      "consistent-return": "off",
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ],
      "no-underscore-dangle": [
        "error",
        {
          "allow": [
            "_id"
          ]
        }
      ],
      "react/react-in-jsx-scope": "off",
      "react/require-default-props": "off",
      "react/prop-types": "off",
      "react/function-component-definition": [
        "error",
        {
          "namedComponents": "arrow-function"
        }
      ],
      "@typescript-eslint/no-unused-vars": [
        "off",
        {
          "argsIgnorePattern": "^_"
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/express-fileupload": "^1.4.1",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/nedb": "^1.8.12",
    "@types/sharp": "^0.31.0",
    "@types/validator": "^13.7.6",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.0",
    "sass": "^1.54.7",
    "source-map-explorer": "^2.5.3"
  }
}
